<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
	<!-- Sequences for student.enroll -->
	<!-- TODO: Need to more detail -->
        <record id="seq_type_student_enroll" model="ir.sequence.type">
            <field name="name">Student Enroll Sequence Type</field>
            <field name="code">student.enroll</field>
        </record>

        <record id="seq_student_enroll" model="ir.sequence">
            <field name="name">Student Enroll Sequence</field>
            <field name="code">student.enroll</field>
            <field name="prefix">STU%(year)s%(month)s%(day)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
